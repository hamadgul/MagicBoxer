
{
  "expo": {
    "newArchEnabled": true,
    "name": "MagicBoxer",
    "displayName": "MagicBoxer",
    "description": "Smart Packaging App.",
    "slug": "magic-packaging-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f2d44"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.magicpackagingapp"
    },
    "android": {
      "package": "com.anonymous.magicpackagingapp"
    }
  }
}